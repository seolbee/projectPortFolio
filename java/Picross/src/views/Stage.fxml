<AnchorPane prefHeight="600.0" prefWidth="450.0" styleClass="background" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="views.StageController">
   <children>
      <Label layoutX="14.0" layoutY="25.0" prefHeight="47.0" prefWidth="175.0" text="스테이지" textFill="WHITE">
         <font>
            <Font size="35.0" />
         </font>
      </Label>
      <ListView fx:id="listView" layoutX="15.0" layoutY="98.0" prefHeight="430.0" prefWidth="229.0" style="-fx-background-color: none;" styleClass="list" />
      <Button layoutX="335.0" layoutY="538.0" mnemonicParsing="false" onAction="#gameStart" prefHeight="47.0" prefWidth="99.0" style="-fx-background-color: none;" text="게임 시작" textFill="WHITE">
         <font>
            <Font name="Arial" size="12.0" />
